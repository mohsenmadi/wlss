/* *** */

tr {
	height: 0px;
}

section {
	position: relative;
	border: 1px solid #bababa;
	padding-top: 34px;
	background: #eee;
}

section.positioned {
	position: absolute;
	top:100px;
	left:100px;
	width:100%;
	padding-bottom: 40px;

	/*box-shadow: 0 0 15px #333;*/
}

#fxhtc {
	overflow-y: auto;
	height: 100%;
}

#fxht {
	border-spacing: 0;
	width:100%;
}

.fxht td + td {
  /*border-left:1px solid #eee;*/
}

.fxht td, .fxht th {
  border-bottom:1px solid #eee;
  background: #fbfbfb;
	font-size: 90%;
	color: #20495e;
	/*padding: 2px 15px 2px 3px;*/
	padding: 3px 2px 3px 2px;
}

.fxht th {
	height: 0px;
	line-height: 0;
	padding-top: -10px;
	padding-bottom: 0px;
	color: transparent;
	border: none;
}

.fxht th .divth{
  position: absolute;
  background: transparent;
	color: #1f6176;
  padding: 9px 25px;
  top: 0;
  margin-left: -25px;
  line-height: normal;
}
.fxht th:first-child .divth{
  border: none;
}

