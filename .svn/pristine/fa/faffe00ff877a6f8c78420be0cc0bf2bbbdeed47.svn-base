#signOut {
	color: #1d588d;
	height: 20px;
	padding-top: 0px;
	font-size: 13px;
	background-color: #adcee9;
	background-image: none;
	border: 1px solid #87b7de;
	border-radius: 4px;
}

#tableLabel {
	font-weight: bold;
}

#copyBtn {
	background-color: transparent;
	border-radius: 4px;
	padding: 0 5px 2px 5px;
	font-size: 12px;
}

#signOut:hover {
	background: #92a9b4;
}

#qrBtn {
	position: relative;
	color: rgb(255, 255, 255);
	background-color: #74C2E1;
	font-size: 11px;
	padding: 2px;
	-webkit-border-radius: 7px;
	-moz-border-radius: 7px;
	border-radius: 7px;
	-webkit-box-shadow: 0px 6px 0px #0191C8, 0px 3px 6px rgba(0, 0, 0, .7);
	-moz-box-shadow: 0px 6px 0px #0191C8, 0px 3px 6px rgba(0, 0, 0, .7);
	box-shadow: 0px 6px 0px #0191C8, 0px 3px 6px rgba(0, 0, 0, .7);
	width: 27px;
}

#qrBtn:active {
	-webkit-box-shadow: 0px 3px 0px #005B9A, 0px 3px 6px rgba(0, 0, 0, .9);
	-moz-box-shadow: 0px 3px 0px #005B9A, 0px 3px 6px rgba(0, 0, 0, .9);
	box-shadow: 0px 3px 0px #005B9A, 0px 3px 6px rgba(0, 0, 0, .9);
	position: relative;
	top: 2px;
}

#qrBtn:disabled, #qrBtn:disabled:hover {
	background: #bbb;
}

#addBtn {
	color: #fff;
	background-color: #72aad8;
	border-color: #2e6da4;
	border: 1px solid transparent;
	border-radius: 4px;
}

#addBtn:disabled, #addBtn:disabled:hover {
	background: #bbb;
}

#addBtn:hover {
	background: #3687c9;
}

#cancelBtn {
	color: #fff;
	background-color: #72aad8;
	border-color: #2e6da4;
	border: 1px solid transparent;
	border-radius: 4px;
}

#cancelBtn:hover {
	background: #3687c9;
}

#searchBtn {
	color: #555555;
	background-color: #deebf5;
	background-image: none;
	border-color: #c8c8d2;
	border-radius: 4px;
}

#searchBtn:hover {
	background: #a7cae4;
}

#userDetails {
	color: #31708f;
	font-size: small;
	padding-top: 15px;
}

#alertBtn {
	font-size: 12px;
	border-radius: 8px;
}

#rowDue {
	color: #666666;
	font-weight: bold;
}

#overDue {
	color: #ff5555;
	font-weight: bold;
}

#notFiledText {
	color: #b15353;
}

#noDues {
	color: #44bb44;
